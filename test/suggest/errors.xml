<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE errors>

<errors>

  <defaults>
    <default type="Grammar error">
      <ids>
        <re v="real-.*"/>
        <e id="unreal-girjji"/>
      </ids>
      <header>
        <title xml:lang="en">Real-word error</title>
        <title xml:lang="se">Čállinmeattáhus</title>
      </header>
      <body>
        <description xml:lang="en">The word "$1" exists in the dictionary, but means something different in this context.</description>
        <description xml:lang="se">"$1" maŋis galggašii leat blah</description>
      </body>
    </default>

    <default type="Grammar error">
      <ids>
        <e id="default"/>
      </ids>
      <header>
        <title xml:lang="en">Grammar error</title>
        <title xml:lang="se">Čállinmeattáhus</title>
      </header>
      <body>
        <description xml:lang="en">The word "$1" exists in the dictionary, but means something <a href="https://divvun.no">different</a> in this context.</description>
        <description xml:lang="se">"$1" maŋis galggašii leat blah</description>
      </body>
    </default>
  </defaults>

  <error id="real-override">
    <header>
      <title xml:lang="en">cake instead of cheese</title>
      <title xml:lang="se">gáhkku iige vuostá</title>
    </header>
    <body>
      <description xml:lang="en">this should be "cake" instead of "cheese"</description>
      <description xml:lang="se">galggašii leat "gáhkku" iige "vuostá"</description>
    </body>
  </error>

  <error id="msyn-compound">
    <header>
      <title xml:lang="en">Compound error</title>
      <title xml:lang="se">Orru leahkime goallossátni</title>
    </header>
    <body>
      <description xml:lang="en">should be one word</description>
      <description xml:lang="se">Orru leahkime goallossátni</description>
    </body>
  </error>

  <error id="msyn-numphrase-sggen">
    <header>
      <title xml:lang="en">Numeralphrase should be in genitive case</title>
      <title xml:lang="se">Tállabáhko ij la rievtes hámen</title>
    </header>
    <body>
      <description xml:lang="en">Long descriprion for the word "$1"</description>
      <description xml:lang="se">Substantiiva "$1", tállabáhko "$2", valla demonstratiiva "$3" la boasto hámen</description>
    </body>
  </error>

  <error id="punct-rihkku">
    <header>
      <title xml:lang="en">Missing comma</title>
      <title xml:lang="se">Rihkku</title>
    </header>
    <body>
      <description xml:lang="en">comma</description>
      <description xml:lang="se">rihkku</description>
    </body>
  </error>

  <error id="msyn-valency-ill">
    <header>
      <title xml:lang="en">illative instead of accusative</title>
      <title xml:lang="se">illatiiva iige akkusatiiva</title>
      <references> <ref n=""/></references>
    </header>
    <body>
      <description xml:lang="en">illative instead of accusative</description>
      <description xml:lang="se">illatiiva iige akkusatiiva</description>
    <examples>
      <ex xml:lang="se">Hui ollu liikon {bohccobierggu} ja {goikebierggu}.</ex>
    </examples>
    </body>
  </error>

  <error id="msyn-sme_interference_neg">
    <header>
      <title xml:lang="en">sme interference neg</title>
      <title xml:lang="se">davvi interference neg</title>
      <references> <ref n=""/></references>
    </header>
    <body>
      <description xml:lang="en">sme interference neg</description>
      <description xml:lang="se">davvi interference neg</description>
    <examples>
      <ex xml:lang="se">Dat ij vuojnnum nav gåk galgaj → Dat ittjij vuojnnu nav gåk galgaj</ex>
    </examples>
    </body>
  </error>

  <error id="msyn-demphrase-congruence-pline">
    <header>
      <title xml:lang="en">pline</title>
      <title xml:lang="se">pline</title>
      <references> <ref n=""/></references>
    </header>
    <body>
      <description xml:lang="en">pline</description>
      <description xml:lang="se">pline</description>
    </body>
  </error>



  <error id="msyn-valency-acc">
    <header>
      <title xml:lang="en">accusative instead of nominative</title>
      <title xml:lang="se">akkusatiiva iige nominatiiva</title>
      <references> <ref n=""/></references>
    </header>
    <body>
      <description xml:lang="en">accusative instead of nominative</description>
      <description xml:lang="se">illatiiva iige nominatiiva</description>
    <examples>
      <ex xml:lang="se">Mun čálán {reive}.</ex>
    </examples>
    </body>
  </error>

  <error id="msyn-valency-loc-com">
    <header>
      <title xml:lang="en">msyn thing</title>
      <title xml:lang="se">msyn thingy</title>
      <references><ref n=""/></references>
    </header>
    <body>
      <description xml:lang="en">wrong word</description>
      <description xml:lang="se">boasttut sátni</description>
    <examples>
      <ex xml:lang="se">Ovdalgihtiigeassima sierra máksingoartta ferte álohii atnit go máksá boastagiro dahje báŋku {badjel}.</ex>
    </examples>
    </body>
  </error>

  <error id="lex-bokte-not-badjel">
    <header>
      <title xml:lang="en">"€1" not "$1"</title>
      <title xml:lang="se">"€1" iige "$1"</title>
      <references><ref n=""/></references>
    </header>
    <body>
      <description xml:lang="en">wrong word, use "€1", not "$1"</description>
      <description xml:lang="se">boasttut sátni, "€1" iige "$1"</description>
    <examples>
      <ex xml:lang="se">Ovdalgihtiigeassima sierra máksingoartta ferte álohii atnit go máksá boastagiro dahje báŋku {badjel}.</ex>
    </examples>
    </body>
  </error>

  <error id="syn-dego-nom">
    <header>
      <title xml:lang="se">dego nom</title>
    </header>
    <body>
      <description xml:lang="se">blah</description>
    </body>
  </error>

  <error id="syn-not-dego">
    <header>
      <title xml:lang="se">not dego</title>
    </header>
    <body>
      <description xml:lang="se">blah</description>
    </body>
  </error>

  <error id="syn-pred-not-attr-msg">
    <header>
      <title xml:lang="se">«$1» ja «$2» leaba blah</title>
    </header>
    <body>
      <description xml:lang="se">Predicative, iige attributive – «$1» ja «$2» leaba blah</description>
    </body>
  </error>

  <error id="msyn-valency-go-inf">
    <header>
      <title xml:lang="en">Try a subclause with <em>go</em> instead of illative</title>
      <title xml:lang="se">Iskka<em>go</em>-oalgecealkaga iige infinitiiva</title>
      <references> <ref n=""/></references>
    </header>
    <body>
      <description xml:lang="en"><em>go</em> instead of illative</description>
      <description xml:lang="se"><em>go</em> iige infinitiiva</description>
    <examples>
      <ex xml:lang="se">Soai leaigga sihke dolkan ja suhttan gullat olbmuid šláddariid sudno birra, ja bovdiiga.</ex>
    </examples>
    </body>
  </error>

  <error id="typo">
    <header>
      <title xml:lang="en">Spelling errors</title>
      <title xml:lang="se">Čállinmeattáhusat</title>
    </header>
    <body>
      <description xml:lang="en">Not in the dictionary</description>
      <description xml:lang="se">Ii leat sátnelisttus</description>
    </body>
  </error>
 
  <error id="space-before-punct-mark">
    <header>
      <title xml:lang="en">Space errors</title>
      <title xml:lang="se">Sátnegaskameattáhusat</title>
    </header>
    <body>
      <description xml:lang="en">There is a space in front of the punctuation mark "$1"</description>
      <description xml:lang="se">Lea gaska "$1" ovddas</description>
    </body>
  </error>

  <error id="syn-phrasal-verb">
    <header>
      <title xml:lang="en">Syntactic error</title>
      <title xml:lang="se">Cealkkameattáhus</title>
    </header>
    <body>
      <description xml:lang="en">The word "$1" exists in the dictionary, but means something different in this context.</description>
      <description xml:lang="se">"$1" maŋis galggašii leat blah</description>
    </body>
  </error>

</errors>
